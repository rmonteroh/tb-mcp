{
  "name": "ticketbeep-mcp",
  "version": "1.0.0",
  "description": "MCP server for TicketBeep API integration",
  "main": "dist/http-mcp-server.js",
  "type": "module",
  "scripts": {
    "build": "tsc && cp .env dist/ 2>/dev/null || echo 'No .env file to copy'",
    "start": "node dist/index.js",
    "start:inspector": "npx -y @modelcontextprotocol/inspector node dist/index.js",
    "start:inspector:no-auth": "npx -y @modelcontextprotocol/inspector DANGEROUSLY_OMIT_AUTH=true node dist/index.js",
    "start:stdio": "node --inspect dist/stdio-mcp-server.js",
    "start:web": "node --inspect dist/http-mcp-server.js",
    "start:dev:stdio": "tsx watch src/stdio-mcp-server.ts",
    "start:dev:web": "tsx watch src/http-mcp-server.ts",
    "test:auth": "tsx test-auth.ts",
    "test:mcp-auth": "tsx test-mcp-auth.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "mcp",
    "ticketbeep",
    "api",
    "media-plan",
    "campaign"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@google/genai": "^1.5.1",
    "@modelcontextprotocol/sdk": "^1.4.0",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "express": "^5.1.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^20.8.0",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.3",
    "tsx": "^4.1.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
